{
  "name": "tyranid-gracl",
  "version": "0.1.16",
  "description": "tyranid.js plugin for gracl",
  "keywords": [
    "graph",
    "acl",
    "permissions",
    "permission",
    "tyranid",
    "gracl"
  ],
  "main": "./dist-node4/src/index.js",
  "typings": "./dist-es6/src/index.d.ts",
  "scripts": {
    "babel": "./node_modules/.bin/babel --quiet ./dist-es6 --out-dir ./dist-node4",
    "build": "rm -rf ./dist-es6/ ./dist-node4/ && npm run lint && npm run tsc && npm run babel && npm run compile-module-test",
    "pretest": "npm run build",
    "prepublish": "npm run test",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "compile-module-test": "cd ./test/tscompile && ../../node_modules/.bin/tsc --pretty",
    "tsc": "./node_modules/typescript/bin/tsc -d --pretty",
    "docs": "rm -rf ./docs/ && ./node_modules/typedoc/bin/typedoc --mode file --theme default --target es6 --out ./docs/ ./src/ && touch ./docs/.nojekyll",
    "gh-pages": "npm run docs && ./node_modules/gh-pages/bin/gh-pages --dotfiles true -d ./docs/",
    "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json ./{test,lib}/**/*.ts && echo '> \\033[0;32mlinter passed!\\033[0m'"
  },
  "engines": {
    "node": ">=4.3.0"
  },
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "4.0.33",
    "@types/lodash": "4.14.36",
    "@types/mongodb": "2.1.33",
    "gracl": "0.0.16",
    "lodash": "4.15.0"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-register": "6.5.2",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "codecov": "1.0.1",
    "gh-pages": "0.10.0",
    "gulp": "3.9.1",
    "mongodb": "2.1.14",
    "nyc": "7.1.0",
    "tslint": "3.5.0",
    "typedoc": "https://github.com/DatenMetzgerX/typedoc.git#typescript-2-build",
    "typescript": "2.0.2",
    "tyranid": "0.1.4",
    "uglifyjs": "2.4.10"
  },
  "peerDependencies": {
    "tyranid": "0.1.4"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "sourceMaps": "inline"
  },
  "ava": {
    "files": [
      "dist-node4/test/spec/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist-node4/src"
    ]
  }
}
