{
  "name": "tyranid-gracl",
  "version": "0.2.4",
  "description": "tyranid.js plugin for gracl",
  "keywords": [
    "graph",
    "acl",
    "permissions",
    "permission",
    "tyranid",
    "gracl"
  ],
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist/ ./test/spec/types && npm run lint && npm run generate-collection-types && npm run tsc && npm run compile-module-test",
    "pretest": "npm run build",
    "prepublish": "npm run test",
    "test": "nyc ava",
    "prettier": "prettier --single-quote true --write './{src,test}/**/*.ts'",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "compile-module-test": "cd ./test/tscompile && tsc --pretty",
    "tsc": "tsc -d --pretty -p ./",
    "docs": "rm -rf ./docs/ && ./node_modules/typedoc/bin/typedoc --mode file --theme default --target es6 --out ./docs/ ./src/ && touch ./docs/.nojekyll",
    "gh-pages": "npm run docs && ./node_modules/gh-pages/bin/gh-pages --dotfiles true -d ./docs/",
    "lint": "./node_modules/tslint/bin/tslint -e ./test/spec/types -c ./tslint.json ./{test,lib}/**/*.ts && echo '> \\033[0;32mlinter passed!\\033[0m'",
    "compile-collections": "tsc -p ./test/models",
    "generate-collection-types": "npm run compile-collections && mkdir -p test/spec/types &&  tyranid-tdgen -o './test/spec/types/isomorphic.d.ts' './dist/test/models/**/*.js' && tyranid-tdgen -o './test/spec/types/server.d.ts' -t server './dist/test/models/**/*.js'"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "4.0.36",
    "@types/lodash": "4.14.69",
    "@types/mongodb": "2.2.7",
    "gracl": "0.0.16",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "codecov": "1.0.1",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "mongodb": "2.2.11",
    "npm-watch": "0.1.6",
    "nyc": "11.0.3",
    "prettier": "1.5.3",
    "tslint": "5.5.0",
    "typedoc": "0.7.1",
    "typescript": "2.4.1",
    "tyranid": "^0.2.11",
    "tyranid-tdgen": "0.2.12"
  },
  "peerDependencies": {
    "tyranid": "^0.2.11"
  },
  "ava": {
    "files": [
      "dist/test/spec/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/src"
    ]
  }
}
