{
  "name": "tyranid-gracl",
  "version": "0.0.0",
  "description": "tyranid.js plugin for gracl",
  "main": "./dist/es5/lib/index.js",
  "typings": "./dist/es6/lib/index.d.ts",
  "scripts": {
    "build": "npm run typings && npm run tscompile && npm run babel",
    "pretest": "npm run build && npm run lint",
    "test": "./node_modules/mocha/bin/mocha dist/es6/test/spec/**/*.js --require babel-polyfill --compilers js:babel-core/register",
    "tscompile": "./node_modules/typescript/bin/tsc -d -p ./",
    "babel":  "./node_modules/.bin/babel ./dist/es6 --out-dir ./dist/es5",
    "typings": "./node_modules/typings/dist/bin/typings.js install",
    "docs": "rm -rf ./docs/ && ./node_modules/typedoc/bin/typedoc --mode file --theme default --experimentalAsyncFunctions --target es6 --out ./docs/ ./lib/ && touch ./docs/.nojekyll",
    "gh-pages": "npm run docs && ./node_modules/gh-pages/bin/gh-pages --dotfiles true -d ./docs/",
    "lint": "./node_modules/tslint/bin/tslint -c ./tslint.json ./{test,lib}/**/*.ts && echo 'linter passed!'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "gracl": "git://github.com/crosslead/gracl.git#d9f11308191c450a82a45f246d8a0379c4c3bffd",
    "lodash": "^3.10.1",
    "tyranid": "0.0.131"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "gh-pages": "^0.10.0",
    "gulp": "^3.9.1",
    "jasmine-core": "^2.4.1",
    "mocha": "^2.4.5",
    "tpmongo": "0.0.6",
    "ts-node": "^0.5.5",
    "tslint": "^3.5.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.7",
    "typings": "^0.6.8",
    "uglifyjs": "^2.4.10"
  }
}
