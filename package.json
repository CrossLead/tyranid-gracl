{
  "name": "tyranid-gracl",
  "version": "0.1.25",
  "description": "tyranid.js plugin for gracl",
  "keywords": [
    "graph",
    "acl",
    "permissions",
    "permission",
    "tyranid",
    "gracl"
  ],
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "watch": {
    "tsc": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "ts"
    },
    "lint": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist/ ./test/spec/generated.d.ts && npm run generate-collection-types && npm run lint && npm run tsc && npm run compile-module-test",
    "pretest": "npm run build",
    "prepublish": "npm run test",
    "test": "nyc ava",
    "watch": "npm-watch",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "compile-module-test": "cd ./test/tscompile && tsc --pretty",
    "tsc": "./node_modules/typescript/bin/tsc -d --pretty -p ./",
    "docs": "rm -rf ./docs/ && ./node_modules/typedoc/bin/typedoc --mode file --theme default --target es6 --out ./docs/ ./src/ && touch ./docs/.nojekyll",
    "gh-pages": "npm run docs && ./node_modules/gh-pages/bin/gh-pages --dotfiles true -d ./docs/",
    "lint": "./node_modules/tslint/bin/tslint -e ./test/spec/generated.d.ts -c ./tslint.json ./{test,lib}/**/*.ts && echo '> \\033[0;32mlinter passed!\\033[0m'",
    "compile-collections": "tsc -p ./test/models",
    "generate-collection-types": "npm run compile-collections && ./node_modules/.bin/tyranid-tdgen './dist/test/models/**/*.js' > test/spec/generated.d.ts"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "4.0.34",
    "@types/lodash": "4.14.40",
    "@types/mongodb": "2.1.35",
    "gracl": "0.0.16",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "codecov": "1.0.1",
    "gh-pages": "0.12.0",
    "gulp": "3.9.1",
    "mongodb": "2.2.11",
    "npm-watch": "0.1.6",
    "nyc": "10.0.0",
    "tslint": "4.0.1",
    "typedoc": "0.5.1",
    "typescript": "2.1.4",
    "tyranid": "0.1.15",
    "tyranid-tdgen": "0.0.19"
  },
  "peerDependencies": {
    "tyranid": "^0.1.15"
  },
  "ava": {
    "files": [
      "dist/test/spec/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/src"
    ]
  }
}
